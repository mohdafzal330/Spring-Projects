    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="student" class="com.codeplanet.beans.StudentBean" p:sname="Mo Abjal" p:saddr="Bhojipura" p:scity="Bareilly" p:sstate="Uttar Pradesh" p:scountry="India" /> 
	<bean id="employee" class="com.codeplanet.beans.Employee" c:ename="Afsar Khan" c:esal="50000" c:eaddr="Bhojipura" c:ecity="Bareilly" c:estate="Uttar Prasesh" c:st-ref="student"/>
	       
        
