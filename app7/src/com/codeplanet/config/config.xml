<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="abjal" class="com.codeplanet.beans.AbjalFamilyData" init-method="init">
		<property name="db_class" value="oracle.jdbc.OracleDriver" />
		<property name="db_driver_url" value="jdbc:oracle:thin:@localhost:1521:xe" />
		<property name="db_username" value="system" />
		<property name="db_pwd" value="Abjal" />
	</bean>        
</beans>